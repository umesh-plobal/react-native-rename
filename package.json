{
  "name": "rename-react-native-android",
  "version": "1.0.0",
  "description": "This the forked project of react-native-rename, It has been modified for spicial usecase to rename only android project. For original package which works for both android and ios, please refer to https://www.npmjs.com/package/react-native-rename",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "dev": "esbuild src/index.js --platform=node --bundle --outdir=lib --external:shelljs --watch",
    "build": "esbuild src/index.js --platform=node --bundle --outdir=lib --external:shelljs --minify --analyze",
    "prepublish": "npm run build",
    "relink": "npm unlink react-native-rename && npm run prepublish && npm link",
    "format": "prettier --write 'src/*.{js,jsx}'",
    "lint": "eslint 'src/*.{js,jsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run format",
      "npm run lint --fix"
    ]
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "rename",
    "renamer",
    "react",
    "space"
  ],
  "author": {
    "email": "umesh.c.ydv@gmail.com",
    "name": "Umesh Yadav"
  },
  "license": "MIT",
  "bin": {
    "react-native-rename": "lib/index.js"
  },
  "devDependencies": {
    "esbuild": "^0.18.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^10.0.1",
    "dotenv": "^16.3.1",
    "globby": "^13.2.0",
    "html-entities": "^2.3.5",
    "normalize-path": "^3.0.0",
    "replace-in-file": "^7.0.1",
    "shelljs": "^0.8.5",
    "update-check": "^1.5.4"
  }
}
